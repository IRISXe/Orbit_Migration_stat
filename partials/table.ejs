<table>
    <thead>
        <tr>
            <% if (data.length > 0) { %>
                <% Object.keys(data[0]).forEach(key => { %>
                    <th><%= key %></th>
                <% }); %>
            <% } %>
        </tr>
    </thead>
    <tbody>
        <% data.forEach(row => { %>
            <tr>
                <% Object.values(row).forEach(value => { %>
                    <td><%= value %></td>
                <% }); %>
            </tr>
        <% }); %>
    </tbody>
</table>
